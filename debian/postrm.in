#include "variables"
#! /bin/bash -e
#
# <% postrm %> script for <:=${PACKAGE}:>
#
<:=@COPYRIGHT:>//

set -o posix

case "${1}" in
    (abort-install|abort-upgrade|failed-upgrade|remove|upgrade)
    :
    ;;
    (disappear|purge)
    <:# purge package from <% debconf %> database :>//
    if test -e <:=${CONFMODULE}:>;
	then source <:=${CONFMODULE}:>; db_version 2.0; db_purge
    fi
    ;;
    (*) echo >&2 "${0##*/}/<:=${PACKAGE}:>:" \
	"Unknown argument: \"${1}\""
    ;;
esac

exit 0
<:
